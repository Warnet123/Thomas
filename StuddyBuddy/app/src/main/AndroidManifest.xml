<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studdybuddy.studdybuddy">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SBLogin" />
        <activity android:name=".SB_Main" />
        <activity android:name=".StuddyBuddyLoad">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SBCalendar" />
        <activity android:name=".SBReview" />
        <activity android:name=".SBRegister" />
        <activity android:name=".SBSettings" />
        <activity
            android:name=".SB_Forum"
            android:label="@string/title_activity_sb__forum" />
        <activity
            android:name=".SB_Calendar"
            android:label="@string/title_activity_sb__calendar" />
        <activity
            android:name=".SB_Review"
            android:label="@string/title_activity_sb__review" />
        <activity
            android:name=".SB_Forumstate2"
            android:label="@string/title_activity_sb__forumstate2"
            android:parentActivityName=".SB_Forum">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.studdybuddy.studdybuddy.SB_Forum" />
        </activity>
        <activity
            android:name=".SBForumSearch"
            android:label="@string/title_activity_sbforum_search"
            android:parentActivityName=".SB_Forum">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.studdybuddy.studdybuddy.SB_Forum" />
        </activity>
    </application>

</manifest>